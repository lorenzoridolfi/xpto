{
    "llm_config": {
        "model": "gpt-3.5-turbo",
        "temperature": 0.7,
        "max_tokens": 1000
    },
    "cache_config": {
        "max_size": 1000,
        "similarity_threshold": 0.8,
        "expiration_hours": 24
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "file": "agent_system.log"
    },
    "agent_settings": {
        "max_retries": 3,
        "timeout": 30,
        "retry_delay": 1
    }
} 